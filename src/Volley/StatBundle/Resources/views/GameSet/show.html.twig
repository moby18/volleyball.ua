{% extends '::base.html.twig' %}

{% block body -%}
    <h1>GameSet</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Scoresethome</th>
                <td>{{ entity.scoreSetHome }}</td>
            </tr>
            <tr>
                <th>Scoresetaway</th>
                <td>{{ entity.scoreSetAway }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ entity.number }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ entity.duration }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('gameset') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('gameset_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
