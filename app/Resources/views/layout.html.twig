{% set title = title|default('Волейбол в Україні') ~ " - volleyball.ua" %}
{% set host = app.request.getSchemeAndHttpHost() %}
{% set route = app.request.attributes.get('_route') %}
{% if route is not empty %}{% set full_url = url(route, app.request.attributes.get('_route_params')) %}{% endif %}
{% set page_image = page_image|default(app.request.getSchemeAndHttpHost() ~ asset('bundles/volleyface/images/fb_sharing_image.jpg')) %}
{% set type = type|default('website') %}
{% set meta_keywords = meta_keywords|default('волейбол, новини волейболу, україна, результати, чемпіонат україни, пляжний волейбол, аматорський волейбол') %}
{% set meta_description = meta_description|default('Новини волейболу, результати матчів, турнірні таблиці, статистика, календарі змагань, команди, волейболісти, аматорський волейбол, пляжний волейбол, фото та відео.') %}
<!DOCTYPE html>
<html lang="en">
<head>

    {% block meta %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
        <meta name="keywords" content="{{ meta_keywords }}" />
        <meta name="description" content="{{ meta_description }}" />
        <meta name="author" content="volleyball.ua@gmail.com">
        <link href="{{ asset('bundles/volleyface/images/favicon.ico') }}" rel="shortcut icon">
        <!-- Fonts -->
        <link href="{{ asset('bundles/volleyface/bower_components/components-font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
        <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

        {% block facebook_meta %}
            <meta property="og:site_name" content="Волейбол в Україні - volleyball.ua" />
            <meta property="og:title" content="{{ title }}" />
            <meta property="og:url" content="{{ full_url }}" />
            {#<meta property="og:image" content="{{ host ~ page_image|apply_filter('facebook_thumb') }}" />#}
            <meta property="og:image" content="{{ page_image }}" />
            <meta property="og:type" content="{{ type }}" />
        {% endblock %}

        {% block twitter_meta %}
            <meta name="twitter:site" content="Волейбол в Україні - volleyball.ua" />
            <meta name="twitter:card" content="summary" />
            <meta name="twitter:title" content="{{ title }}" />
            <meta name="twitter:url" content="{{ full_url }}" />
            <meta name="twitter:description" content="{{ meta_description }}" />
            <meta name="twitter:image" content="{{ page_image }}" />
        {% endblock %}

        {% block vk_meta %}
            <link rel="image_src" href="{{ page_image }}">
        {% endblock %}
    {% endblock %}

    <title>{{ title }}</title>

    <!-- Bootstrap Core CSS -->
    {% block styles %}
        <link href='{{ asset('css/style.css') }}' rel="stylesheet" type='text/css'>
        <!--[if lt IE 9]>
        <link href='{{ asset('css/style_ie.css') }}' rel="stylesheet" type='text/css'>
        <![endif]-->
    {% endblock %}

    {% block head_scripts %}
        <!-- IE8 support for HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="{{ asset('js/script_ie.js') }}" type="text/javascript"></script>
        <![endif]-->
    {% endblock %}

    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
