{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "beberlei/doctrineextensions": "^1.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/migrations": "^2.0",
        "doctrine/orm": "^2.6.4",
        "friendsofsymfony/user-bundle": "~2.1.2",
        "gedmo/doctrine-extensions": "~2.3.9",
        "hwi/oauth-bundle": "^0.6.3",
        "incenteev/composer-parameter-handler": "^2.1.3",
        "knplabs/knp-menu": "^2.0",
        "knplabs/knp-menu-bundle": "^2.3",
        "knplabs/knp-paginator-bundle": "^2.4",
        "liip/imagine-bundle": "^2.2",
        "mopa/bootstrap-bundle": "^3.3",
        "oneup/uploader-bundle": "^2.1.4",
        "php-http/guzzle6-adapter": "1.1.1",
        "php-http/httplug-bundle": "^1.16",
        "presta/sitemap-bundle": "^1.7",
        "sensio/framework-extra-bundle": "^5.4",
        "stfalcon/tinymce-bundle": "2.0",
        "stof/doctrine-extensions-bundle": "~1.3",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2.8",
        "symfony/symfony": "^4.3",
        "symfony/templating": "^4.3",
        "tetranz/select2entity-bundle": "^2.10",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^4.3"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    }
}
